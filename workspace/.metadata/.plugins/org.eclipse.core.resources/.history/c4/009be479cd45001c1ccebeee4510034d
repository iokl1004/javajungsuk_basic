
public class Hello {

	public static void main(String[] args) {
		// ctrl+shift+L 단축키 전체 목록보기
		// ctrl+ +, - 글자 크기
		// ctrl_D 한줄 삭제
		// ctrl+alt+down 행단위 복사
		// alt+shift+A 멀티컬럼편집
		// alt+up, down 행단위 이동
		System.out.println("Hello,world.");
		System.out.println("Hello,world.");
		System.out.println("Hello,world.");
		System.out.println("Hello,world.");
		System.out.println("Hello,world.");	
	}

}